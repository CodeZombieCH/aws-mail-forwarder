{
    "fromEmail": "forwarder@your-domain.tld",
    "subjectPrefix": "",
    "allowPlusSign": true,
    "forwardMapping": {
        "john@your-domain.tld": [
            "john.doe@some-other-domain.tld"
        ],
        "jane@your-domain.tld": [
            "jane.doe@some-other-domain.tld"
        ],
        "@your-domain.tld": [
